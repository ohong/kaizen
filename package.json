{
  "name": "langgraph-python-starter",
  "version": "0.1.0",
  "private": true,
  "description": "Kaizen analyses developer impact and offers an actionable theory of improvement",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:ui\" \"npm run dev:agent\" --names ui,agent --prefix-colors blue,green --kill-others",
    "dev:debug": "LOG_LEVEL=debug npm run dev",
    "dev:agent": "cd agent && npx @langchain/langgraph-cli dev --port 8123 --no-browser",
    "dev:ui": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "install:agent": "sh ./scripts/setup-agent.sh || scripts/setup-agent.bat",
    "postinstall": "npm run install:agent"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ohong/kaizen.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ohong/kaizen/issues"
  },
  "homepage": "https://github.com/ohong/kaizen#readme",
  "dependencies": {
    "@ag-ui/langgraph": "0.0.7",
    "@ai-sdk/openai": "^1.3.22",
    "@copilotkit/react-core": "1.9.3",
    "@copilotkit/react-ui": "1.9.3",
    "@copilotkit/runtime": "1.9.3",
    "@supabase/supabase-js": "^2.58.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "autoprefixer": "^10.4.21",
    "next": "15.3.2",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "recharts": "^3.2.1",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.9.3",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@langchain/langgraph-cli": "0.0.40",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.1.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
